{
    "name": "codexsoft/operations",
    "license": "MIT",
    "description": "A tool provides building blocks for structuring domain business-logic via operations",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^7.2.0",
        "ext-mbstring": "*",
        "codexsoft/cli": "^1.0",
        "codexsoft/code-classes": "^1.0",
        "codexsoft/code-files": "^1.0",
        "codexsoft/code-strings": "^1.0",
        "codexsoft/shortcut-tab-constant": "^1.0",
        "codexsoft/time-service": "^1.0",
        "ramsey/uuid": "^3.8",
        "symfony/console": "~3.4|~4.0|~5.0",
        "symfony/event-dispatcher": "^4.1|~5.0",
        "symfony/filesystem": "~3.4|~4.0|~5.0",
        "voku/stringy": "^6.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "CodexSoft\\OperationsSystem\\": ["src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": ["vendor/", "tests/unit/"]
        }
    },
    "bin": [
        "bin/codexsoft.operations"
    ]
}
